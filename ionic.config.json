{
  "defaultProject": "crypto-app",
  "projects": {
    "crypto-app": {
      "name": "crypto-app",
      "integrations": {
        "capacitor": {
          "ios": {
            "contentInset": "automatic"
          },
          "android": {
            "allowMixedContent": true
          }
        }
      },
      "type": "angular-standalone",
      "framework": "@angular",
      "bundler": "webpack",
      "webDir": "./dist/crypto-app/browser",
      "build": {
        "options": {
          "sourceMap": true,
          "optimization": {
            "scripts": true,
            "styles": true,
            "fonts": true
          }
        }
      }
    }
  }
}